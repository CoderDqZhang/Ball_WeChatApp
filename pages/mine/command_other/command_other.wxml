<!--pages/mine/command_other/command_other.wxml-->
<view class='startsession'>
  <view class='title'>球品评价</view>
  <view class="right">
    <view bindtap="userStarTap" data-index="{{index}}" class="star" wx:for="{{userStars}}" wx:key="index">
      <image src="{{item}}"></image>
    </view>
  </view>
</view>
<view class='startsession'>
  <view class='title'>球技评价</view>
  <view class="right">
    <view bindtap="gameStarTap" data-index="{{index}}" class="star" wx:for="{{gameStars}}" wx:key="index">
      <image src="{{item}}"></image>
    </view>
  </view>
</view>
<input placeholder='请输入评价' class='search_input' bindinput='input_game_subtitle' placeholder-style='font-size:28rpx;color:#DDE0E5' style='font:28rpx; color:#384269 border-radius: 10rpx; bordr-color:#384269; width:100%; align:right;text-align:left;font-size:28rpx;height:200rpx; margin-left:30rpx'
  confirm-type='done' value='{{game.game_place_condition}}'></input>
<button class='appointment_btn' bindtap='appointment_btn_press'>评价</button>